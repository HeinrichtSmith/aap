# Terminal System Configuration
# Copy this file to .env and update with your actual values

# WebSocket endpoint for real-time communication
VITE_TERMINAL_WS_URL=ws://localhost:8080/ws

# HTTP API endpoint (used as fallback if WebSocket fails)
VITE_TERMINAL_API_URL=http://localhost:8080/api

# Authentication token (optional)
# VITE_TERMINAL_AUTH_TOKEN=your-auth-token-here

# Other configuration options
VITE_TERMINAL_RECONNECT_INTERVAL=5000
VITE_TERMINAL_MAX_RECONNECT_ATTEMPTS=5