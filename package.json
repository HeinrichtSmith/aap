{
  "name": "arrowhead-polaris",
  "version": "6.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "terminal-server": "node mock-terminal-server.cjs",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "npm run dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"BACKEND,FRONTEND\" --prefix name",
    "start:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"BACKEND,FRONTEND\" --prefix name",
    "start:safe": "node preflight-check.cjs && npm run dev:all",
    "preflight": "node preflight-check.cjs",
    "restart:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"BACKEND,FRONTEND\" --prefix name",
    "stop": "stop-services.bat",
    "restart": "stop-services.bat && npm run restart:all",
    "verify": "verify-services.bat"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "delaunator": "^5.0.1",
    "framer-motion": "^10.16.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.20.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-window": "^1.8.11",
    "recharts": "^2.10.0",
    "reveal.js": "^5.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0",
    "ws": "^8.18.3"
  }
}